1    file1=open("sample.txt","r+")
2    file1_read=file1.readlines()
3    count = 0
4    for line in file1_read:
5        count += 1
6        print("Line{}: {}".format(count, line.strip()))
7    file1.close()
8    
9    filename = "welcome.txt"
10   
11   try:
12       with open(filename, "r") as file:
13           print(file.read())
14   except FileNotFoundError:
15       print(f"Error: The File '{filename}'was not found.")
